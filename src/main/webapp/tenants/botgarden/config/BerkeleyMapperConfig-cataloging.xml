<bnhmmaps>
    <metadata>
        <name>Point Verification</name>
        <abstract>File to do a quick verification of key points</abstract>
        <mapkeyword keyword="controls"/>
        <defaultscreensize value="800"/>
    </metadata>
    <colors method="dynamicfield" fieldname="char120_1" label="Location Description">
        <dominantcolor webcolor="629ad3"/>
        <subdominantcolor webcolor="ddddff"/>
    </colors>
    <annotation show="1">
        <annotation_replyto_email value="foobar@foo.com"/>
    </annotation>
    <themes>
        <theme method="colorprecision" fieldname="darwin:coordinateuncertaintyinmeters"/>
    </themes>
    <settings>
        <setting name="landsat" show="1"/>
        <setting name="opaque" show="1"/>
        <setting name="colorprecision" show="1"/>
    </settings>
    <concepts xmlns:darwin="http://digir.net/schema/conceptual/darwin/2003/1.0">
        <concept order="9" viewlist="1" colorlist="0" datatype="darwin:decimallatitude" alias="DecimalLatitude"/>
        <concept order="9" viewlist="1" colorlist="0" datatype="darwin:decimallongitude" alias="DecimalLongitude"/>
        <concept order="9" viewlist="1" colorlist="0" datatype="darwin:coordinateuncertaintyinmeters" alias="CoordinateUncertaintyInMeters"/>
        <concept order="20" viewlist="1" colorlist="1" datatype="darwin:horizontaldatum" alias="HorizontalDatum"/>
        <concept order="3" viewlist="1" colorlist="1" datatype="char120_1" alias="Scientific Name"/>
        <concept order="4" viewlist="1" colorlist="1" datatype="char120_2" alias="Date"/>
        <concept order="5" viewlist="1" colorlist="1" datatype="char120_3" alias="Specific Locality"/>
        <concept order="6" viewlist="1" colorlist="1" datatype="char120_4" alias="Collection Number"/>        
    </concepts>
   </bnhmmaps>